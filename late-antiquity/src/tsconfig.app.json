{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "outDir": "../out-tsc/app",
    "baseUrl": "./",
    "module": "esnext",
    "target": "es5",
    "types": ["jquery"],
    "paths": {
      "@angular/*": [
        "../node_modules/@angular/*"
      ],
      "jquery/*": [
        "../node_modules/jquery/*"
      ]
    }
  },
  "angularCompilerOptions": {
    "enableIvy": false
  },
  "exclude": [
    "test.ts",
    "**/*.spec.ts"
  ]
}
